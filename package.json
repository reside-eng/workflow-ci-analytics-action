{
  "name": "@side/workflow-ci-analytics-action",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "husky",
    "format": "biome format --write",
    "format-check": "biome format",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "build:docs": "tsx bin/generate-docs.ts",
    "build": "rimraf dist && ncc build src/index.ts && yarn build:docs",
    "test": "vitest",
    "test:cov": "yarn test --coverage",
    "types:check": "tsc --noEmit",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.0",
    "@google-cloud/bigquery": "7.9.1",
    "@types/node": "22.10.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.6.1",
    "@side/commitlint-config": "1.1.0",
    "@vercel/ncc": "^0.38.3",
    "@vitest/coverage-v8": "2.1.8",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "pinst": "3.0.0",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "tsx": "^4.19.2",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  },
  "lint-staged": {
    "*.{json,md,yaml,yml,js,ts}": [
      "biome check --no-errors-on-unmatched --write"
    ]
  },
  "packageManager": "yarn@4.5.3"
}
